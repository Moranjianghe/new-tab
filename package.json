{
  "name": "new-tab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "vercel": "vue-tsc -b && cross-env VITE_BUILD_TIME=\"$(date)\" vite build",
    "preview": "vite preview",
    "extension": "vue-tsc -b && vite build --mode extension",
    "githubIo": "vue-tsc -b && vite build --base=/my-new-tab/"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.6",
    "js-yaml": "^4.1.0",
    "nvm": "^0.0.4",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.6",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vue-tsc": "^2.2.8"
  }
}
